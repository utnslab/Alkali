event MY_EVENT {
    atom at;
    context ctx;
}

handler MY_EVENT:my_handler_event (context ctx, int c, int d) {
    bits<32> result;
    bits<1> cond;
    bits<1> cond2;
    result = 1;
    cond = c == 1;
    cond2 = d == 1;
    if (cond) {
        cond = 0;
    } else {
        result = 233;
    }
    result = result + 2;
    if (cond2) {
        ctx.const = 2;
        ctx.name = result;
    }
}
